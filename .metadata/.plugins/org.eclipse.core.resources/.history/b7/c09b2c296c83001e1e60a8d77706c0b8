<script>
	const pcode = "[[${param.pcode}]]"; //thymleaf에서 파라미터값 받아오는 방법
	
	$(window).on("load", function(){
		$.ajax({
			type:"get",
			url:"/pro/read.json",
			data:{pcode},
			success:function(data){
				console.log(data);
			}
		})
	})
</script>