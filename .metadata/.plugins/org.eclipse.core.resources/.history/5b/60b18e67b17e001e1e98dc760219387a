<div class="my-5">
	<h1 class="text-center mb-5">게시글</h1>
	<div id="div_posts">
		
	</div>
</div>

<script id="temp_posts" type="x-handlebars-template">
	<table class="table">
	{{#each .}}
		<tr>
			<th>{{title}}</th>
		</tr>
	{{/each}}
	</table>
</script>

<script>
	getPosts();
	
	function getPosts(){
		$.ajax({
			type:"get",
			url:"/posts/list.json",
			success:function(data){
				//console.log(data);
				const temp=Handlebars.compile($("#temp_posts").html());
				$("#div_posts").html(temp(data));
			}
		})
	}
</script>